from fanstatic import Group
from fanstatic import Library
from fanstatic import Resource


library = Library("{{package}}", "static")

css = Resource(
    library,
    "styles.css",
    minified="styles.min.css")
js = Resource(
    library,
    "scripts.js",
    minified="scripts.min.js")

css_and_js = Group([css, js])
